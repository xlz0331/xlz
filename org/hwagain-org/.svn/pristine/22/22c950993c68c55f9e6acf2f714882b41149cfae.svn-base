package com.hwagain.org.register.dto;

import java.util.List;

import com.hwagain.org.dept.dto.OrgDeptProDto;
import com.hwagain.org.user.dto.OrgUserJobDto;

/**
 * 岗位所有上级信息
 * @author wangct
 *
 */
public class RegJobUpperDto {

	/**
	 * 所有上级部门
	 */
	private List<OrgDeptProDto> allUpperDepts;
	/**
	 * 上级人员岗位
	 */
	private String leaderJob;

	/**
	 * 分管范围
	 */
	private List<OrgDeptProDto> managDepts;
	/**
	 * 所有直接上级人员
	 */
	private List<OrgUserJobDto> leaders;
	
	public List<OrgDeptProDto> getManagDepts() {
		return managDepts;
	}

	public void setManagDepts(List<OrgDeptProDto> managDepts) {
		this.managDepts = managDepts;
	}
	
	public List<OrgUserJobDto> getLeaders() {
		return leaders;
	}

	public void setLeaders(List<OrgUserJobDto> leaders) {
		this.leaders = leaders;
	}

	public List<OrgDeptProDto> getAllUpperDept() {
		return allUpperDepts;
	}

	public void setAllUpperDept(List<OrgDeptProDto> allUpperDepts) {
		this.allUpperDepts = allUpperDepts;
	}

	public String getLeaderJob() {
		return leaderJob;
	}

	public void setLeaderJob(String leaderJob) {
		this.leaderJob = leaderJob;
	}

}
