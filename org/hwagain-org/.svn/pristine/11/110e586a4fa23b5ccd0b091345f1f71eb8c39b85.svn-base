package com.hwagain.org.job.dto;

import java.util.ArrayList;
import java.util.List;

public class OrgJobMenu {

	private String nodeCode;//节点编号
	
	private String nodeName;//节点名称
	
	private String paramsJson;//请求参数
	
	private Integer requestType;//请求方式
	
	private String typeCode;//节点类型
	
	private String companyCode;
	
	private List<OrgJobMenu> children = new ArrayList<OrgJobMenu>();
	
	public OrgJobMenu() {
		super();
	}
	
	public OrgJobMenu(String nodeCode, String nodeName, String paramsJson, Integer requestType, String typeCode, String companyCode) {
		super();
		this.nodeCode = nodeCode;
		this.nodeName = nodeName;
		this.paramsJson = paramsJson;
		this.requestType = requestType;
		this.typeCode = typeCode;
		this.companyCode = companyCode;
	}

	public String getNodeCode() {
		return nodeCode;
	}

	public void setNodeCode(String nodeCode) {
		this.nodeCode = nodeCode;
	}

	public String getNodeName() {
		return nodeName;
	}

	public void setNodeName(String nodeName) {
		this.nodeName = nodeName;
	}

	public String getParamsJson() {
		return paramsJson;
	}

	public void setParamsJson(String paramsJson) {
		this.paramsJson = paramsJson;
	}

	public Integer getRequestType() {
		return requestType;
	}

	public void setRequestType(Integer requestType) {
		this.requestType = requestType;
	}

	public String getTypeCode() {
		return typeCode;
	}

	public void setTypeCode(String typeCode) {
		this.typeCode = typeCode;
	}

	public String getCompanyCode() {
		return companyCode;
	}

	public void setCompanyCode(String companyCode) {
		this.companyCode = companyCode;
	}

	public List<OrgJobMenu> getChildren() {
		return children;
	}

	public void setChildren(List<OrgJobMenu> children) {
		this.children = children;
	}
}
